{
  "name": "Chrome-Jumpy",
  "description": "Jumpy inspired extension - select element based on a combination of keys.",
  "version": "0.1.0",
  "manifest_version": 3,
  "permissions": ["storage", "tabs", "scripting", "activeTab"],
  "host_permissions": ["http://*/", "https://*/"],
  "action": {
    "default_icon": "assets/chrome_jumpy_icon.png",
    "default_popup": "dist/index.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": "Ctrl+M",
      "default": "Ctrl+M"
    }
  }
}